{
    "name": "yiisoft/yii-masked-input",
    "description": "Yii Framework Masked input widget Extension",
    "keywords": ["yii", "masked input", "inputmask"],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-masked-input/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-masked-input"
    },
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii-jquery": "^3.0@dev",
        "npm-asset/inputmask": "~3.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "dev-master",
        "yiisoft/cache": "dev-master",
        "yiisoft/yii-web": "dev-master",
        "hiqdev/composer-config-plugin": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\widgets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\maskedinput\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php"
        }
    }
}
