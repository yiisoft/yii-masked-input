{
    "name": "yiisoft/yii-masked-input",
    "description": "Yii Framework Masked input widget Extension",
    "keywords": [
        "yii",
        "masked input",
        "inputmask"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-masked-input/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "chat": "https://t.me/yii3en",
        "source": "https://github.com/yiisoft/yii-masked-input"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "npm-asset/inputmask": "~5.0.0",
        "yiisoft/yii-jquery": "^3.0@dev"
    },
    "require-dev": {
        "npm-asset/jquery": "*",
        "phan/phan": "^5.4",
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/composer-config-plugin": "dev-master",
        "yiisoft/di": "^1.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\MaskedInput\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\MaskedInput\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "tests": "tests.php"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
